{
  "keys": {
    "s": {
      "name": "shell",
      "keys": {
        "b": "SHELL=/bin/bash bash",
        "z": "SHELL=/bin/zsh zsh",
        "f": "SHELL=/usr/bin/fish fish"
      }
    },
    "g": {
      "name": "go",
      "loopingKeys": ["t"],
      "keys": {
        "b": "go build .",
        "i": "make install",
        "t": "go test ."
      }
    }
  }
}
